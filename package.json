    "android:reload": "adb shell input text 'RR'",
    "devtools": "adb reverse tcp:8097 tcp:8097 && react-devtools &",
    "postinstall": "patch-package",
    "abhishek:android": "adb devices && adb -s ZY322DDWPJ reverse tcp:8081 tcp:8081 && react-native run-android",

    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "prettier-fix": "prettier --write **/*.js",
    "lint": "prettier --config ./.prettierrc --write 'src/{,!(node_modules)/**/}*.js'",
    "prettier-check": "prettier --list-different **/*.js",
    "lint-fix": "eslint --fix .",
